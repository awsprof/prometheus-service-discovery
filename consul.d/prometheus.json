{
    "service": {
        "name": "prometheus",
        "address": "prometheus",
        "port": 9090,
        "tags": [
            "monitoring"
        ],
        "checks": [
            {
                "id": "metrics",
                "name": "Prometheus Server Metrics",
                "http": "http://prometheus:9090/metrics",
                "interval": "10s"
            }
        ]
    }
}